@model List<Vaccine>
<h2>@ViewData["Name"]</h2>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x" crossorigin="anonymous">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-gtEjrD/SeCtmISkJkNUaaKMoLD0//ElJ19smozuHV6z3Iehds+3Ulb9Bn9Plx0x4" crossorigin="anonymous"></script>
<a href="/Vaccines/AddVaccineView">New Vaccine</a>  |  <a href="/Vaccines/AddNewDosesView">New Doses</a>
<table class="table table-bordered table table-sm">
    <thead>
        <tr>
            <th>Vaccine</th>
            <th>Doses Required</th>
            <th>Days Between Doses</th>
            <th>Total Doses Received</th>
            <th>Total Doses Left</th>
            <th>Edit</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var vaccine in Model)
        {

        <tr>
            <td>@vaccine.Name</td>
            <td>@vaccine.DosesRequired</td>
            <td>@vaccine.DaysBetweenDoses</td>
            <td>@vaccine.TotalDosesReceived</td>
            <td>@vaccine.TotalDosesLeft</td>
            <td><a href="/Vaccines/EditVaccine/@vaccine.Id">Edit</a></td>

        </tr>
        }
    </tbody>
</table>